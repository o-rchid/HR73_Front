	{
	if(deleteEmp==null){
		this.alert("사원을 선택해주세요.")
		}else{
			this.transaction("deleteEmpEval",
							 "svcEmpinfomgmt::removalevaluation",
							 "",
							 "",
							 "empCode="+deleteEmp+" applyDay="+this.gfn_getYear(this.gfn_today()),
